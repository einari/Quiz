<?xml version="1.0" encoding="utf-8"?>
<ServiceManifest xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" Name="ReceiverPkg" Version="1.0.0" xmlns="http://schemas.microsoft.com/2011/01/fabric">
   <ServiceTypes>
      <StatelessServiceType ServiceTypeName="ReceiverType"/>
   </ServiceTypes>
   <CodePackage Name="Code" Version="1.0.0">
      <EntryPoint>
         <ExeHost>
            <Program>entryPoint.sh</Program>
            <ConsoleRedirection FileRetentionCount="5" FileMaxSizeInKb="2048"></ConsoleRedirection>
         </ExeHost>
      </EntryPoint>
   </CodePackage>
   <ConfigPackage Name="Config" Version="1.0.0" />
   <Resources>
      <Endpoints>
         <Endpoint Name="ServiceEndpoint" />
      </Endpoints>
   </Resources>
</ServiceManifest>
